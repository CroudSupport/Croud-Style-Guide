{
    "croud-box-shadow-subtle": {
        "less": "0px 1px 2px 0 fade(@croud-colour-border, 15%)",
        "sass": "0px 1px 2px 0 rgba($croud-colour-border, .15)"
    },
    "croud-box-shadow-float": {
        "less": "0px 2px 4px 0px fade(@croud-colour-border, 12%), 0px 2px 10px 0px fade(@croud-colour-border, 15%)",
        "sass": "0px 2px 4px 0px rgba($croud-colour-border, .12), 0px 2px 10px 0px rgba($croud-colour-border, .15)"
    }
}
