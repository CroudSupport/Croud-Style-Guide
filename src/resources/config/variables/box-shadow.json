{
    "croud-box-shadow-subtle": {
        "less": "0px 1px 2px 0 fade(@croud-colour-border, 20%)",
        "sass": "0px 1px 2px 0 rgba($croud-colour-border, .20)"
    },
    "croud-box-shadow-float": {
        "less": "0px 2px 2px 0px fade(@croud-colour-border, 12%), 0px 2px 8px 1px fade(@croud-colour-border, 15%)",
        "sass": "0px 2px 2px 0px rgba($croud-colour-border, .12), 0px 2px 8px 1px rgba($croud-colour-border, .15)"
    }
}
