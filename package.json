{
  "name": "croud-style-guide",
  "version": "1.9.0",
  "description": "A Vue.js project",
  "author": "antony.king@croud.co.uk",
  "private": false,
  "scripts": {
    "semantic:build": "gulp --cwd semantic/ build",
    "styleguide:watch": "gulp --cwd src/resources/",
    "styleguide:build": "cd src/resources/build/ && node build",
    "tech": "vue-styleguidist server",
    "tech:dev": "yarn run styleguide:watch | yarn run tech",
    "tech:build": "vue-styleguidist build",
    "dev:styleguide": "yarn run styleguide:watch | node build/dev-server.js",
    "dev": "node build/dev-server.js",
    "build": "node build/build.js && node checkUpdates",
    "test": "./node_modules/.bin/jest -i",
    "lint": "eslint --ext .js,.vue src test/unit/specs",
    "stylelint:staged": "node node_modules/stylelint/bin/stylelint",
    "precommit": "lint-staged"
  },
  "files": [
    "semantic/",
    "src/components/shared/",
    "src/resources/",
    "README.md",
    "CONTRIBUTING.md"
  ],
  "lint-staged": {
    "*.vue": "stylelint:staged"
  },
  "jest": {
    "moduleNameMapper": {
      "@/components/shared/(.*).$": "<rootDir>/src/components/shared/$1.vue"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!croud-vue-semantic|vue-quill)"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    },
    "setupFiles": [
      "<rootDir>/config/test.env.js"
    ],
    "verbose": true
  },
  "dependencies": {
    "clipboard": "^1.7.1",
    "croppie": "^2.5.0",
    "croud-date-parser": "git+https://github.com/CroudSupport/croud-date-parser.git",
    "pikaday": "^1.6.1",
    "tool": "^6.0.0",
    "vuetable-2": "^1.7.0"
  },
  "devDependencies": {
    "@mapbox/stylelint-processor-arbitrary-tags": "^0.1.1",
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.3",
